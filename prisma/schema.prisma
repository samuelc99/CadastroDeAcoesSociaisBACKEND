generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Action {
  id          Int      @id @default(autoincrement())
  title       String
  responsible String
  description String
  location    String
  contact     String   // <<< precisa existir
  link        String?
  createdAt   DateTime @default(now())
}
